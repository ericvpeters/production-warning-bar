{
  "name": "production-warning-bar",
  "version": "0.1.2",
  "description": "A chrome extension that detects production pages",
  "devDependencies": {
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babelify": "~7.3.0",
    "eslint": "~3.11.1",
    "eslint-plugin-react": "^6.8.0",
    "material-ui": "^0.16.4",
    "nodeunit": "~0.10.2",
    "q": "~1.4.1",
    "react": "^15.4.1",
    "react-color": "^2.9.0",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "watchify": "~3.7.0"
  },
  "scripts": {
    "test": "nodeunit test/**/*.js",
    "eslint": "./node_modules/.bin/eslint src/js/**/*.js*",
    "watch": "./script/watch.sh",
    "build": "./script/build.sh",
    "package": "npm run jshint && npm test && npm run build && zip package-$(cat package.json | jq -r .version).zip -r build manifest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarkil/production-warning-bar.git"
  },
  "keywords": [
    "chrome",
    "extensions",
    "production"
  ],
  "author": "Tarkil",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tarkil/production-warning-bar/issues"
  },
  "homepage": "https://github.com/tarkil/production-warning-bar#readme"
}
